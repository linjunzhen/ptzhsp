@charset "utf-8";

body,dd,div,dl,dt,form,html,img,li,ol,ul {
	margin: 0;
	padding: 0
}

input,select,textarea {
	vertical-align: middle;
	padding: 0;
	margin: 0
}

textarea {
	resize: none
}

img {
	border: 0;
	vertical-align: middle
}

li,ol,ul {
	list-style: none
}

a {
	color: #000;
	text-decoration: none
}

a:hover {
	color: #00479d;
	text-decoration: none;
}

body {
	font-family: "微软雅黑",sans-serif;
	height: 100%;
