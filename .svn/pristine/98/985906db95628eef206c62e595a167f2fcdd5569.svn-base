.eui-nav {float: left;overflow: hidden;padding: 0;height: 72px;margin-left: 10px;}

.eui-nav dt {float: left;height: 72px;line-height: 72px;font-weight: 400;position: relative;}

.eui-nav dt a span {float: left;display: inline-block;word-wrap: break-word;word-break: break-all;text-align: center;width: 80px;}

.eui-nav dt a .wzspan {width: 69px;line-height: 72px;text-align: center;}

.eui-nav dt a {display: block;height: 72px;padding: 0 10px;font-size: 16px;color: #d3e5ff;width: 80px;outline: none;}

.eui-nav dt a label {
display: inline-block;
width: 10px;
height: 5px;
margin: 30px 12px 0 12px;
background: url(../images/arrow.png) no-repeat top
}

.eui-nav .open a label {
background: url(../images/arrow.png) no-repeat bottom
}

.eui-nav dt a.on,
.eui-nav .open a,
.eui-nav dt a:hover,
.eui-nav .on span,
.eui-nav .open span, 
.eui-nav dt span:hover {
	color:#fff;
}

.eui-nav .on span::after,.eui-nav .open span::after,.eui-nav dt span:hover::after{
	content:'';
	width:30px;
	position:absolute;
	bottom:14px;
	background: #fff;
	height: 3px;
	left: 35px;
}


.eui-nav dt a img {
float: left;
width: 36px;
height: 36px;
margin: 16px 6px 0 0;
background: url(../images/img-bg.png) no-repeat
}

.eui-nav .open a img,.eui-nav dt a:hover img,.eui-system ul .on a,.eui-system ul li a:hover {
background: url(../images/imgon-bg.png) no-repeat
}

