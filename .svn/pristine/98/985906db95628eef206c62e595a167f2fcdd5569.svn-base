	margin: 0 auto
}

table {
	border-collapse: collapse;
	border-spacing: 0
}

td,th {
	padding: 0
}

*html {
	overflow: -moz-scrollbars-vertical;
	zoom: expression(function(ele){ele.style.zoom = "1";
	document.execCommand("BackgroundImageCache",false,true)
}
(this))
}

