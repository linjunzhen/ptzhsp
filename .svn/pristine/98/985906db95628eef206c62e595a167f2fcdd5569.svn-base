/* 修改框架样式代码 */
.layout-panel-west .panel-header{
	padding:0px;
	height: 100%;
	line-height: 0px;
	color: #014781;
	/* background-color: #fdfeff; */
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#d5dde8,endColorstr=#d5dde8,GradientType=0);
	border: 0;
	z-index: 2;
	background: transparent;
}
.layout-panel-west .panel-header .panel-title{
	font-size: 16px;
	font-weight: 400;
	padding-left: 16px;
	color: #014781;
	background-color: #fdfeff;
	height: 56px;
	line-height: 56px;
}
.layout-panel-west .panel-header  .panel-tool a,.layout-expand-west .panel-header  .panel-tool a {
  display: inline-block;
  width: 16px;
  height: 16px;
  opacity: 0.6;
  filter: alpha(opacity=100);
  margin: 0 0 0 2px;
  vertical-align: top;
}
.layout-panel-west .panel-header  .panel-tool a:hover,.layout-expand-west .panel-header  .panel-tool a:hover {
  opacity: 1;
  filter: alpha(opacity=100);
  background-color: transparent;
  -moz-border-radius: 3px 3px 3px 3px;
  -webkit-border-radius: 3px 3px 3px 3px;
  border-radius: 3px 3px 3px 3px;
}
.layout-expand,.layout-expand-over {
    background-color:#d5dde8 !important;
}
.layout-expand-west .panel-header,.layout-expand-west  .panel-body{border:none;}
