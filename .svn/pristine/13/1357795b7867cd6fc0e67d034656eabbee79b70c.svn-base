body {
	background: #f0f0f0;
}


.eui-content {
	height: auto;
	padding-bottom: 100px;
}

/*内容开始*/
.slideTxtBox {
	text-align: left;
	overflow: auto;
	
}

.slideTxtBox .hd {
	height: 66px;
	border-bottom: solid 1px #cecece;
	position: relative;
}

.slideTxtBox .hd ul {
	display: flex;
	align-items: center;
	height: 100%;
}

.slideTxtBox .hd ul li {
	display: flex;
	align-items: center;
	height: 100%;
	margin: 0 34px;
	cursor: pointer;
	font-size: 20px;
	color: #000000;
}

.slideTxtBox .hd ul li.on {
	font-weight: 600;
	border-bottom: solid 4px #1658a0;
	color: #1658a0;
	box-sizing: border-box;
}

.slideTxtBox .bd ul {
	zoom: 1;
}

.slideTxtBox .bd li {
	height: 24px;
	line-height: 24px;
}

.slideTxtBox .bd li .date {
	float: right;
	color: #999;
}

.eui-table-input {
	margin-top: 17px;
	padding: 0 35px;
}

.eui-table-input table {
	width: 100%;
	font-size: 14px;
}

.eui-table-input table tr {
	margin-bottom: 10px;
	display: inline-block;
}

.eui-table-input table tr td {
	height: 40px;
	line-height: 20px;
	font-size: 16px;
	color: #333333;
}

.eui-table-input table tr td span {
	color: #ff0000;
}

.eui-table-input table tr td:nth-child(odd) {
	width: 150px;
	text-align: right;
	padding-right: 10px;
	/* vertical-align: text-top; */
}

.eui-table-input table tr td input {
	width: 455px;
	height: 40px;
	line-height: 40px;
	border: solid 1px #d4d4d4;
	box-sizing: border-box;
	padding: 0 10px;
}

.eui-table-input table tr td select {
	width: 455px;
	height: 37px;
	line-height: 37px;
	border: solid 1px #d4d4d4;
	appearance: none;
	-moz-appearance: none;
	-webkit-appearance: none;
	background: url(../images/new/down.png) no-repeat right, #FFFFFF;
	background-position-x: 430px;
	padding-left: 10px;
	box-sizing: border-box;
}

