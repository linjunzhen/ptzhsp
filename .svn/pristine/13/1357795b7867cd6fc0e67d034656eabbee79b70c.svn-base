.eui-table-input .info{
	display: flex;
	align-items: center;
}
.eui-table-input .eui-jiaoyan {
	display: inline-block;
	width: 60px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	background: #3a81d1;
	color: #FFFFFF;
	margin: 0 10px;
}

.eui-table-input b {
	color: #ff0000;
	font-weight: 500;
}

.eui-table-input .eui-a-href {
	color: #ff0000;
	text-decoration: underline;
}

.eui-table-input .eui-bh {
	width: 455px;
}

.eui-table-input table tr td .eui-input-h {
	height: 60px;
}

.eui-table-input table tr td .eui-input-w {
	width: 1070px;
	height: 60px;
}

.eui-9-progress-btn {
	position: absolute;
	left: 50%;
	transform: translate(-50%);
	display: inline-block;
	width: 90px;
	height: 36px;
	text-align: center;
	line-height: 36px;
	font-size: 16px;
	color: #FFFFFF;
	background: #0f88ec;
	border-radius: 5px;
	margin-top: 16px;
}





.eui-9-progress {}

.eui-9-progress-head {
	height: 36px;
	margin: 20px 30px 0 30px;
	position: relative;
}

.eui-9-progress-head span {
	display: inline-block;
	width: 300px;
	height: 36px;
	line-height: 36px;
	color: #FFFFFF;
	font-size: 18px;
	background: url(../images/index3.png) no-repeat;
	background-size: 100%;
	padding-left: 20px;
}

.eui-9-progress-head a {
	display: inline-block;
	height: 36px;
	line-height: 36px;
	font-size: 16px;
	color: #FFFFFF;
	background: #0f88ec;
	text-align: center;
	padding: 0 15px;
	border-radius: 5px;
	position: absolute;
	right: 0;
	top: 50%;
	transform: translateY(-50%);
}

.eui-9-progress-input {
	margin-top: 18px;
	position: relative;
}

.eui-9-progress-input table {
	padding: 0 60px;
	display: inline-block;
}

.eui-9-progress-input tr {
	display: inline-block;
	margin-top: 6px;
}

.eui-9-progress-input tr td {
	height: 37px;
	line-height: 37px;
}

.eui-9-progress-input table tr td:nth-child(odd) {
	width: 140px;
	text-align: right;
	padding-right: 10px;
	vertical-align: text-top;
}

.eui-9-progress-input tr td input {
	width: 350px;
	height: 35px;
	line-height: 35px;
	border: solid 1px #e1e6eb;
}

.eui-9-progress-input table tr td select {
	width: 352px;
	height: 37px;
	line-height: 37px;
	border: solid 1px #e1e6eb;
	appearance: none;
	-moz-appearance: none;
	-webkit-appearance: none;
	background: url(../images/down.png) no-repeat right, #FFFFFF;
	background-position-x: 320px;
	padding-left: 10px;
}

.eui-9-progress-input table tr .eui-9-progress-data select {
	width: 160px;
}

.eui-9-progress-input table tr .eui-9-progress-data span {
	display: inline-block;
	width: 23px;
	text-align: center;
}

.eui-9-progress-btn {
	position: absolute;
	/*bottom: 0;*/
	left: 50%;
	transform: translate(-50%);
	display: inline-block;
	width: 90px;
	height: 36px;
	text-align: center;
	line-height: 36px;
	font-size: 16px;
	color: #FFFFFF;
	background: #0f88ec;
	border-radius: 5px;
	margin-top: 16px;
}

.eui-9-progress-table-1 {
	margin: 60px 30px 30px 30px;
}

.eui-9-progress-table-1 table {
	width: 100%;
}

.eui-9-progress-table-1 table tr:first-child {
	background: -webkit-linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* Firefox 3.6 - 15 */
	background: linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* 标准的语法 */
}

.eui-9-progress-table-1 table tr {
	text-align: center;
}

.eui-9-progress-table-1 table tr th {
	height: 41px;
	line-height: 41px;
	color: #FFFFFF;
	font-size: 16px;
	border: solid 1px #e1e1e1;
}

.eui-9-progress-table-1 table tr td {
	height: 60px;
	line-height: 60px;
	color: #4c4c4c;
	font-size: 16px;
	border: solid 1px #e1e1e1;
}

.eui-9-progress-table-1 table tr td .span-1 {
	color: #01b549;
}

.eui-9-progress-table-1 table tr td .span-2 {
	color: #f40000;
}

.eui-9-progress-table-3 {
	margin: 30px;
}

.eui-9-progress-table-3 table {
	width: 100%;
}

.eui-9-progress-table-3 table tr {
	height: 60px;
	line-height: 60px;
}

.eui-9-progress-table-3 table tr td {
	font-size: 14px;
	border: solid 1px #e1e1e1;
}

.eui-9-progress-table-3 table tr td:nth-child(odd) {
	width: 184px;
	text-align: right;
	padding-right: 16px;
	color: #FFFFFF;
	background: -webkit-linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* Firefox 3.6 - 15 */
	background: linear-gradient(to right, rgba(15, 136, 236, 0.8), rgba(62, 158, 238, 0.8));
	/* 标准的语法 */
}

.eui-9-progress-table-3 table tr td:nth-child(even) {
	padding-left: 20px;
}

.czxq {
	color: #0078ff;
	text-decoration: underline;
}



/*10 项目登记*/
.eui-9-progress-table-title i {
	display: block;
	font-weight: 600;
	font-size: 18px;
	color: #0068b7;
	margin: 20px 0 10px 0;
}

.eui-10-progress {
	margin: 10px 30px 30px 30px;
}

.eui-10-progress table {
	table-layout: fixed;
}

.eui-10-progress table tr th:nth-child(1),
.eui-10-progress table tr td:nth-child(1) {
	display: inline-block;
	width: 130px;
}

.eui-10-progress table tr th:nth-child(2),
.eui-10-progress table tr td:nth-child(2) {
	display: inline-block;
	width: 578px;
}

.eui-10-progress table tr th:nth-child(3),
.eui-10-progress table tr td:nth-child(3) {
	display: inline-block;
	width: 364px;
}

.eui-10-progress a {
	display: inline-block;
	width: 90px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	font-size: 16px;
	color: #4c4c4c;
	border-radius: 5px;
	margin: 0 10px;
}

.eui-10-progress a.sc {
	border: solid 1px #939393;
}

.eui-10-progress a.xz {
	border: solid 1px #0582e1;
	color: #0582e1;
}

.eui-10-progress a.shanchu {
	border: solid 1px red;
	color: red;
}

