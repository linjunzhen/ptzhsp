package net.evecom.core.model;

import net.evecom.platform.base.model.BaseModel;

/**
 * 
 * 描述
 * @author Flex Hu
 * @version 1.0
 * @created 2015年10月9日 上午9:32:34
 */
public class FileFolder extends BaseModel {
    /**
     * 目录名称
     */
    private String folderName;
    /**
     * 目录路径
     */
    private String folerPath;

    public FileFolder() {

    }

    public FileFolder(String folderName, String folerPath) {
        this.folderName = folderName;
        this.folerPath = folerPath;
    }

    public String getFolderName() {
        return folderName;
    }

    public void setFolderName(String folderName) {
        this.folderName = folderName;
    }

    public String getFolerPath() {
        return folerPath;
    }

    public void setFolerPath(String folerPath) {
        this.folerPath = folerPath;
    }
}
