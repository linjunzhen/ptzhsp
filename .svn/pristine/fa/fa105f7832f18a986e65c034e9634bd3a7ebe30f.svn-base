@charset "utf-8";
/* CSS Document */
/*  通用样式  */
html,body,div,form,img,ul,ol,li,dl,dt,dd{margin:0px; padding:0px;}
select,input,textarea{font-size:12px; vertical-align:middle; padding:0px; margin:0px;}
textarea{resize:none;}
img{border:0;}
ol,ul{list-style:none;}
a{color:#000; text-decoration:none;}
a:hover{color:#3188d2;}
