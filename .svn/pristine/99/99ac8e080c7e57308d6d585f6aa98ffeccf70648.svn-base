<input type="hidden" name="DSBGQNR"  value="${busRecord.DSBGQNR }">
<input type="hidden" name="DSBGHNR"  value="${busRecord.DSBGHNR }">							
<input type="hidden" name="JSBGQNR"  value="${busRecord.JSBGQNR }">
