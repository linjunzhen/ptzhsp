<div id="basxxzDiv">
	<div style="position:relative;">
		<table cellpadding="0" cellspacing="1" class="tab_tk_pro2">
			<tr>
				<th colspan="4">备案选择</th>
			</tr>
			<tr>
				<th style="width: 10%;">是否备案</th>
				<th style="width: 20%;">备案事项</th>
				<th style="width: 35%;">备案前内容</th>
				<th style="width: 35%;">备案后内容</th>
			</tr>
			<tr>
				<td style="text-align: center;">
					<input class="checkbox validate[required]"  name="BASX" value="1" type="checkbox">
				</td>
				<td>董事备案</td>
				<td id="DSBGQNR_TD">${busRecord.DSBGQNR }</td>
				<td id="DSBGHNR_TD">${busRecord.DSBGHNR }</td>
