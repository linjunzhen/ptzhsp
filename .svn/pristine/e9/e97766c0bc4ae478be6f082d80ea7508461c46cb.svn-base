@charset "utf-8";
/* CSS Document */
/*  通用样式  */
html,body,div,form,img,ul,ol,li,dl,dt,dd{margin:0px; padding:0px;}
select,input,textarea{font-size:12px; vertical-align:middle; padding:0px; margin:0px;}
textarea{resize:none;}
img{border:0;}
ol,ul{list-style:none;}
a{color:#000; text-decoration:none;}
a:hover{color:#3188d2;}
body{font-family:"宋体",sans-serif; font-size:12px; height:100%; width:100%; background:url(../images/bodybg.png) no-repeat center top #fff; min-width:1000px; margin:0 auto;word-break: break-all;}
.lfloat{float:left;}
.rfloat{float:right;}
.Ctext{text-align:center;}
.Ltext{text-align:left;}
.Rtext{text-align:right;}
.lbmargin{margin:20px 0;}
/*下面是对联广告的css代码*/
.eve_ad{position:absolute;z-index:9999;overflow:hidden; display:none;}
.eve_ad_con{border:#CFCFCF solid 1px;overflow:hidden;}
.eve_ad_close{ width:100%; height:24px; line-height:24px; text-align:right; display:block; font-size:13px; color:#555555; text-decoration:none;}
/*清除浮动*/
.clearfix:after{content:'\0020';display:block;height:0;clear:both;visibility:hidden;}
.clearfix{*zoom:1; _zoom:1;}
.container{width:1000px; margin:0 auto;}
.lbpadding{padding:0 0 10px 0;}
.lbpadding20{padding:20px 0;}
.tmargin10{margin-top:10px;}
.tmargin12{margin-top:12px;}
