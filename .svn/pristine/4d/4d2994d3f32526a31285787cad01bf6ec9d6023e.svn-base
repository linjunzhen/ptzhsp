.eui-9-progress-table-title i{display: block;font-weight: 600;font-size: 18px;color: #0068b7;margin:20px 0 10px 0;}
.eui-10-progress{margin:10px 30px 30px 30px;}
.eui-10-progress table{table-layout: fixed;}
.eui-10-progress table tr th:nth-child(1),.eui-10-progress table tr td:nth-child(1){display: inline-block;width: 128px;}
.eui-10-progress table tr th:nth-child(2),.eui-10-progress table tr td:nth-child(2){display: inline-block;width: 578px;}
.eui-10-progress table tr th:nth-child(3),.eui-10-progress table tr td:nth-child(3){display: inline-block;width: 364px;}
.eui-10-progress a{display: inline-block;width: 90px;height: 30px;line-height: 30px;text-align: center;font-size:16px;color: #4c4c4c;border-radius: 5px;margin:0 10px;}
.eui-10-progress a.sc{border: solid 1px #939393;}
.eui-10-progress a.xz{border: solid 1px #0582e1;color: #0582e1;}
.eui-10-progress a.shanchu{border: solid 1px red;color: red;}
