.eui-content{
	height: auto;
	padding-bottom: 100px;
	background: none;
}
/*内容开始*/
.slideTxtBox{/*border:1px solid #ddd;*/ border-top: none; text-align:left; /*margin: 0 20px; height: 50.5vh;*/overflow: auto;padding-bottom: 10px; margin-bottom: 20px;}
.slideTxtBox .hd{ height:50px; line-height:50px; background:none;  position:relative; }
.slideTxtBox .hd ul{ float:left;  position:absolute; left:0px; top:-1px; height:32px;   }
.slideTxtBox .hd ul li{ float:left; padding:0 15px; cursor:pointer;  font-size: 20px; color: #0582e1;}
.slideTxtBox .hd ul li.on{ height:44px; line-height: 40px;font-weight: 600;  background:#cae7fd; border:1px solid #ddd;     border-bottom: solid 2px rgba(255,255,255,0.1);border-top:6px solid #0068b7; color: #0068b7; font-style: italic;}
/* .slideTxtBox .bd{ border: 1px solid #ddd;} */
.slideTxtBox .bd ul{  zoom:1;  }    
.slideTxtBox .bd li{ height:24px; line-height:24px;   }
.slideTxtBox .bd li .date{ float:right; color:#999;  }

.eui-table-input{
	/* font-family: "宋体"; */
	/* margin:20px 0; */
/* 	text-align: center; */
}
.eui-table{
	border-bottom: solid 2px #3a81d1;
	box-sizing: border-box;
	margin: 0 30px 10px 30px;
}
.eui-table p{
	height: 50px;
	text-align: left;
	line-height: 50px;
	font-size: 18px;
	color: #3a81d1;
}


.eui-table-input table{
	width: calc(100% - 60px);
	margin: 0 30px;
	font-size: 14px;
	/* width: 1000px; */
}
.eui-table-input table tr{
}
.eui-table-input table tr td{
	height: 40px;
	line-height: 40px;
	padding: 5px 0;
	font-size: 16px;
}
.eui-table-input table tr td span{
	color: #ff0000;
}
.eui-table-input table tr td:nth-child(odd){
	width: 160px;
	text-align: right;
	padding-right: 10px;
	vertical-align: text-top;
}
.eui-table-input table tr td:nth-child(4){
	width: 400px;
}
.eui-table-input table tr td input{
	width: 280px;
	height: 40px;
	line-height: 40px;
	border: solid 1px #c9deef;
	box-sizing: border-box;
	padding: 0 10px;
}

.eui-table-input table tr td div input{
	width: 350px;
	height: 16px;
	line-height: 16px;
	border: solid 1px #c9deef;
}

.eui-table-input table tr td select{
	width: 280px!important;
	height: 40px;
	line-height: 40px;
	border: solid 1px #c9deef;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	background: url(../images/down.png) no-repeat right,#FFFFFF;
	background-position-x: 255px;
	padding-left: 10px;
	box-sizing: border-box;
}
