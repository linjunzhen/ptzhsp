/* 基本样式 */
body {
	width: 100%;
	height: 100%;
}
input, button, select, textarea {
	font-family: inherit;
	font-size: inherit;
	line-height: inherit;
	outline: none;
}
a {
	text-decoration: none;
}
a:hover, a:focus {
	text-decoration: none;
}
figure {
	margin: 0;
}
img {
	vertical-align: middle;
	border: none;
}
ol, ul, li {
	list-style: none;
	margin: 0px;
	padding: 0px;
}
textarea {
	resize: vertical;
}
.lfloat {
	float: left;
}
.rfloat {
	float: right;
}
/*IE下a:hover 背景闪烁*/
*html {
	overflow: -moz-scrollbars-vertical;

	_zoom:expression(function(ele) {
		ele.style.zoom ="1";
		document.execCommand("BackgroundImageCache", false, true)
	}
	(this))
}

/*清除浮动*/
.clearfix:after {
	content: '';
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.clearfix {
	*zoom: 1;
	_zoom: 1;
}
a {
	outline: none;
}
/* 通用 */
.clearfloat:after {
    clear: both;
    content: "";
    display: block;
    height: 0;
    visibility: hidden;
}
.sc {
	overflow: hidden;
}

/* 五彩麒麟APP start */
.eui-page {
	position: absolute;
	width: 100%;
	height: 100%;
}
.eui-contPd {
	padding: 0 12px;
}
.eui-pg-bg {
	background: #fff;
}
.eui-ftprnts li {
	position: relative;
	padding-left: 26px;
	min-height: 86px;
}
.eui-lst-line {
	display: block;
	position: absolute;
	left: 14px;
	top: 0;
	bottom: 0;
	width: 2px;
	background: #eee;
}
.eui-lst-dot {
	display: inline-block;
	position: absolute;
	top: 16px;
	left: 12px;
	width: 6px;
	height: 6px;
	border-radius: 3px;
	background: #1c97ff;
	z-index: 1;
}
.eui-lst-cont {
	padding: 10px 0 8px;
	border-bottom: 1px solid #eee;
}
.eui-ftprnt-text1 {
	margin: 0;
	line-height: 20px;
	font-size: 13px;
	color: #999;
}
.eui-ftprnt-text2 {
	margin: 0;
	line-height: 25px;
	font-size: 15px;
	color: #444;
}
.eui-ftprnt-text3 {
	margin: 0;
	line-height: 23px;
	font-size: 13px;
	color: #999;
}

.eui-tabs {
	height: 39px;
	border-bottom: 1px solid #eee;
}
.eui-tabs.col4 li {
	float: left;
	width: 25%;
	height: 38px;
	line-height: 38px;
	font-size: 13px;
	color: #444;
	text-align: center;
}
.eui-tabs.col4 li.on {
	color: #36acff;
	border-bottom: 2px solid #36acff;
}
.eui-tabCont {
	background: #eee;
}
.eui-dealEvt {
	margin-bottom: 6px;
	background: #fff;
}
.eui-dealEvt-infos {
	padding: 10px 24px 8px;
}
.eui-dealEvt-text1 {
	margin: 0;
	line-height: 24px;
	font-size: 12px;
	color: #666;
}
.eui-dealEvt-text2 {
	margin: 0;
	line-height: 24px;
	font-size: 12px;
	color: #444;
	font-weight: bold;
}
.eui-dealEvt-text3 {
	margin: 0;
	line-height: 24px;
	font-size: 11px;
	color: #999;
}
.eui-dealEvt-stat {
	float: right;
	line-height: 32px;
	font-size: 13px;
	color: #38adff;
}
.eui-dealEvt-stat.done {
	color: #f39800;
}
.eui-dealEvt-btm {
	padding: 10px 24px;
	background: #fafafa;
	overflow: hidden;
}
.eui-dealEvt-btn1,
.eui-dealEvt-btn2,
.eui-dealEvt-btn3 {
	display: inline-block;
	margin-left: 8px;
	padding: 0 11px;
	height: 20px;
	line-height: 18px;
	font-size: 12px;
	border-radius: 10px;
	text-decoration: none;
	box-sizing: border-box;
}
.eui-dealEvt-btn1 {
	color: #444;
	border: 1px solid #444;
}
.eui-dealEvt-btn2 {
	color: #38adff;
	border: 1px solid #38adff;
}
.eui-dealEvt-btn3 {
	color: #f39800;
	border: 1px solid #f39800;
}

.eui-materials {
	background: #e5e5e5;
}
.eui-material {
	margin-bottom: 5px;
	padding-top: 8px;
	background: #fff;
}
.eui-material.edit {
	position: relative;
	z-index: 10;
}
.eui-material-tt {
	margin: 0 23px;
	padding: 9px 0;
	line-height: 17px;
	font-size: 12px;
	color: #444;
}
.eui-material-tt .import {
	color: #e60012;
}
.eui-attachments {
	position: relative;
	margin-left: 23px;
	height: 96px;
}
.eui-material-upload,
.eui-material-delete {
	float: left;
	position: absolute;
	left: 0;
	top: 0;
	padding: 3px 0;
	margin-right: 10px;
	width: 80px;
}
.eui-pic-box {
	display: block;
	height: 60px;
	box-sizing: border-box;
}
.eui-material-upload .eui-pic-box,
.eui-material-delete .eui-pic-box {	
	border: 1px dashed #ddd;
	background: #efefef;
}
.eui-pic-box i {
	display: block;
	height: 100%;
}
.eui-material-upload .eui-pic-box i {
	background: url('../images/upload.png') no-repeat 50%;
	background-size: 36px 29px;
}
.eui-material-delete .eui-pic-box i {
	background: url('../images/delete.png') no-repeat 50%;
	background-size: 21px 23px;
}
.eui-pic-text {
	display: block;
	line-height: 26px;
	font-size: 10px;
	color: #444;
	text-align: center;
}

.eui-attachment {
	margin-left: 90px;
	height: 100%;
	font-size: 0;
	overflow-x: auto;
	white-space: nowrap;
}
.eui-attachment-item {
	display: inline-block;
	margin-right: 6px;
	padding: 3px;
	width: 90px;
	box-sizing: border-box;
}
.eui-attachment-item.on {
	position: relative;
	border: 2px solid #38adff;
}
.eui-attachment-item.on:after {
	content: "";
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	width: 15px;
	height: 15px;
	background: url('../images/attachment_on.png') no-repeat;
	background-size: contain;
}
.eui-attachment-item .eui-pic-box img {
	display: block;
	width: 80px;
	height: 60px;
}

.eui-evaluation {
	padding: 25px 42px;
	text-align: center;
}

.eui-eval-text1 {
	margin: 0;
	line-height: 40px;
	font-size: 20px;
	color: #444;
	text-align: center;
}
.eui-eval-text2 {
	display: inline-block;
	margin: 0;
	line-height: 26px;
	font-size: 13px;
	color: #999;
	text-align: center;
}
.eui-eval-line {
	display: block;
	margin: 11px 15px 0;
	border-bottom: 1px solid #ddd;
}
.eui-eval-scores {
	margin: 20px auto 0;
	width: 220px;
	height: 26px;
}
.eui-eval-scores a {
	float: left;
	width: 44px;
	height: 26px;
	background: url('../images/agree.png') no-repeat;
	background-size: contain;
}
.eui-eval-scores a.on {
	background: url('../images/agree_on.png') no-repeat;
	background-size: contain;
}
.eui-eval-tags {
	margin: 27px -4px 0;
	font-size: 0;
	overflow: hidden;
}
.eui-eval-tags a {
	display: inline-block;
	margin: 0 4px 17px;
	padding: 0 20px;
	height: 25px;
	line-height: 25px;
	font-size: 13px;
	color: #38adff;
	border-radius: 18px;
	background: #eaf7fd;
}
.eui-eval-tags a.on {
	color: #fff;
	background: #38adff;
}
.eui-eval-advs {
	display: block;
	margin-top: 8px;
	padding: 8px 17px;
	width: 100%;
	height: 100px;
	line-height: 20px;
	font-size: 13px;
	color: #38adff;
	border: 0;
	border-radius: 6px;
	background: #eaf7fd;
	box-sizing: border-box;
	resize: none;
}
.eui-form-sub {
	margin-top: 9px;
	text-align: left;
	overflow: hidden;
}
.eui-check {
	display: inline-block;
	height: 26px;
	line-height: 26px;
	font-size: 13px;
	color: #38adff;
}
.eui-check i {
	float: left;
	margin-right: 7px;
	width: 21px;
	height: 26px;
	background: url('../images/check.png') no-repeat;
	background-size: contain;
}
.eui-check.on i {
	background: url('../images/check_on.png') no-repeat;
	background-size: contain;
}
.eui-full-btn {
	display: block;
	margin-top: 20px;
	height: 40px;
	line-height: 40px;
	font-size: 13px;
	color: #fff;
	border-radius: 6px;
	background: #38adff;
	box-shadow: 0 3px 2px #b2e7ff;
	text-align: center;
}

.eui-search-his,
.eui-search-hot {
	margin: 0 24px;
	padding: 22px 0;
}
.eui-search-his {
	margin-bottom: 4px;
	border-bottom: 1px solid #ddd;
}
.eui-search-tt {
	margin: 0;
	height: 24px;
	line-height: 24px;
	font-size: 15px;
	font-weight: normal;
	color: #444;
}
.eui-search-clr {
	float: right;
	color: #999;
}
.eui-search-tags {
	margin: 10px -6px 0;
	overflow: hidden;
}
.eui-search-tag {
	float: left;
	margin: 0 6px 8px;
	padding: 0 12px;
	height: 30px;
	line-height: 28px;
	font-size: 12px;
	color: #444;
	border: 1px solid #ddd;
	border-radius: 3px;
	text-decoration: none;
	box-shadow: 0 1px 1px #ddd;
	box-sizing: border-box;
}
.eui-search-tag.isHot {
	color: #f39800;
}
.eui-mask {
	display: none;
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: rgba(255, 255, 255, 0.9);
	z-index: 1;
}
.eui-page-notice {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	height: 50px;
	line-height: 50px;
	font-size: 13px;
	color: #fff;
	background: rgba(0, 0, 0, 0.6);
	text-align: center;
	transition: all 3s ease;
}


