@charset "utf-8";
/* CSS Document */
html,body{height: 100%;}
html,body,div,form,img,ul,ol,li,dl,dt,dd{margin:0px; padding:0px;}
body{margin:0 auto;padding:0;border:0; font-family:"微软雅黑"; font-size: 14px;}
select,input,textarea{vertical-align:middle; padding:0; margin:0;}
textarea{resize:none;}
img{border:0;}
ol,ul,li{list-style:none;}
a{text-decoration:none; transition: ease-in-out .3s;}
a,img,input,textarea,select,button,area{outline:none;}
*{-webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;}
/*清除浮动*/
.clearfix:after{content:'';display:block;height:0;clear:both;visibility:hidden;}
.clearfix{*zoom:1; _zoom:1;}
.lfloat{float:left;}
.rfloat{float:right;}
table{border-collapse:collapse;border-spacing:0;}
td,th{padding:0;}
::-webkit-scrollbar-track { background-color: none;  }
::-webkit-scrollbar {  width: 0px;height:8px;  background-color:none;border-radius: 5px;  }
::-webkit-scrollbar-thumb {background: none;border-radius: 5px; }
::-webkit-scrollbar-corner{ background-color: none;}



body{font-family: "微软雅黑"; color: #ffffff;}
.cn{text-align: center;}
.tm10{margin-bottom: 10px!important;}
.tm20{margin-bottom: 20px!important;}
.tm30{margin-bottom: 30px!important;}
.tp20{padding-top: 10px!important;}
.eui-wrapper{width: 1920px; padding: 20px; height: 1080px; background: url(../images/bg.png) center no-repeat; background-size: 100% 100%;}
/*头部*/
.eui-head{position:relative; width: 100%; height: 59px; text-align:center; margin: 0 auto 25px; background: url(../images/logo-bg.png) center no-repeat; }
.eui-head img{margin:0 auto;}
.returnbtn{display:block; padding-right:10px; width:110px; height: 40px; line-height:40px; font-size: 14px; color: #fff; background: rgba(73,141,255,0.1) url(../images/ljt-icon.png) no-repeat; background-position: 70px 12px; border-radius: 30px; position: absolute; left: 20px;; top: 10px;opacity: 0.8;}
.returnbtn:hover{opacity: 1;}
.city-select {position: absolute; left: 50%; top: 10px; margin-left:200px; padding-left: 30px;}
.city-select:before{content: '/'; position: absolute; left: 0; top: 0; font-size: 24px; color: #fff;}
.city-select select {color: #fff; border:none; height: 37px; appearance:none; -moz-appearance:none; -webkit-appearance:none;background:url(../images/select-icon.png) no-repeat scroll right center transparent; padding-right:30px;}
.city-select select option,.time-select select option{color: #26BEFF;}
.time-select{ position: absolute; right: 20px;; top: 10px; height: 40px; width: 105px;}
.time-select select { width: 100%; padding: 0 30px; color: #fff; border:none; height: 40px; appearance:none; -moz-appearance:none; -webkit-appearance:none;background:url(../images/select-icon.png) no-repeat scroll right center rgba(73,141,255,0.1);  border-radius: 10px; background-position:90% 50% ;}
.tt{width: 100%; height: 39px; line-height: 39px; font-size: 22px; color: #fff; text-align: center; background: url(../images/tt-bg.png) center no-repeat;}
.eui-con{padding: 0 20px;}
.conL,.conR{width: 470px;}
.conC{width: 860px; margin: 0 20px;}

/*首页样式*/
/*tab切换*/
.eui-tabl,.eui-tabr{width: 216px; height: 57px; display: block; position: absolute;top: 0px;line-height: 57px;font-family: '微软雅黑';font-size: 28px; color: #9cbaf2; text-align: center;box-sizing: border-box;}
.eui-tabl.on,.eui-tabl:hover{background: url(../images/tabl-on.png) center no-repeat; background-size: 100%; color: #fff;}
.eui-tabr.on,.eui-tabr:hover{background: url(../images/tabr-on.png) center no-repeat; background-size: 100%; color: #fff;}
.eui-tabl{left: 50%; margin-left:-540px; background: url(../images/tabL-bg.png) center no-repeat;}
.eui-tabr{right: 50%; margin-right:-540px; background: url(../images/tabR-bg.png) center no-repeat;}

.Ltop,.Lbtm,.Cbtm,.Rtop,.Rcon,.Rbtm,.sbox,.conCL,.conCR,.fbox,.sfbox,.sfRT,.jdrtj,.jdjgtj,.jdkh,.jdjcqk,.fjyzLT,.fjyzLC,.fjyzLB,.fjzyRT,.flyzjg,.ajlx{border: 1px solid rgba(73,141,255,0.3); margin-bottom: 20px;}

 /*行政立法*/
.xzlf,.flyw,.ggflfw,.sqjztable,.sfjdyw{padding: 20px 20px 0;}
.xzlf ul li{width: 50%; float: left; margin-bottom: 30px;}
.xzlf ul li img{width: 70px; height: 66px; float: left; margin-right: 15px;}
.xzlf ul li span>span{display:block; color: #26beff; font-size: 16px;}
.xzlf ul li em{font-style:normal; font-size: 16px; color: rgba(255,255,255,0.5);}
.xzlf ul li em b{font-size: 32px; color: #fff; margin-right: 5px;}

/*法律业务*/
.flyw ul li{display: table; width: 100%; padding: 10px 20px; background: rgba(73,141,255,0.1); margin-bottom: 6.5px; border: 2px solid transparent;}
.flyw ul li:hover{ background: rgba(21,71,181,0.3); border: 2px solid #498dff;}
.flyw ul li span{display: table-cell; vertical-align: middle; color: #fff; line-height: 28px; padding-right: 5px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; width: 20%;}
.flyw ul li .td:first-child{width: 20%;}
.flyw ul li span>b{display: block;font-style: normal; font-size: 20px;}
.flyw ul li .sjx{margin-left: 10px;}
.sjx{width: 6px; height: 6px;border-style: solid; display: inline-block; vertical-align: middle; margin-top: -2px; }
.red{ border-width: 6px 6px 0; border-color:#e44a45 transparent transparent;}
.green{border-width: 0 6px 6px; border-color:transparent transparent #39bc7a;}
.blue{border-width: 6px 0 6px 6px; border-color:transparent transparent  transparent #26beff;}
.flyw ul li span>em{display: block; font-style: normal; font-size: 12px; color: #26beff;}
/*公共法律服务*/
.Ctop{margin-bottom: 20px;}
.ggflfw{width:100%; border: solid rgba(73,141,255,0.3);border-width:0 1px 1px 1px; }
.itt{position: relative; border-top: 1px solid rgba(73,141,255,0.3); width: calc(100% - 88px); height: 44px; margin: 0 auto; background: url(../images/tt-bg.png) top center  no-repeat;}
.itt:before{content: ''; position: absolute; left: -44px; top: 0; bottom: 0; width: 44px; height: 44px; background: url(../images/itt-bg.png) center no-repeat;}
.itt:after{content: ''; position: absolute; right: -44px; top: 0; bottom: 0; width: 44px; height: 44px; background: url(../images/itt-bg.png) center no-repeat; transform: rotate(90deg);}
.ggflfw ul li{float: left; width: 18%; background: url(../images/li-bg.png) left bottom no-repeat; padding: 5px 0; margin: 0 1%;}
.ggflfw ul li>span{display:block; font-size: 16px; color: #26beff;}
.ggflfw ul li>span i{margin-right: 5px;}
.ggflfw ul li>b{display:block; font-size: 32px; color: #FFFFFF;}
.ggflfw ul li>b i{display:inline-block; width: 20px; height: 20px;}
.rup{background: url(../images/gdown.png) center no-repeat;}
.gdown{background: url(../images/rup-icon.png) center no-repeat;}
.gmap{position: relative;margin-top: 15px;}
.gmap .map{width: 565px; height: 516px;}
.gmap .dszxpm{position:absolute; right: 0; top: 40px; width: 338px;}
.gmap .dszxpm .line{ position:relative; border-top: 1px solid rgba(73,141,255,0.3); margin: 0;}
.gmap .dszxpm .line:before{content: ''; position: absolute; left: 0; top: -2px; width: 5px; height: 3px; background: #0ac3f8;}
.gmap .dszxpm .line:after{content: ''; position: absolute; right: 0; top: -2px; width: 5px; height: 3px; background: #0ac3f8;}
.pmbox{background: rgba(73,141,255,0.3); margin-top: 5px; padding: 0 20px;}
.pmbox>p{font-size: 18px; color: #fff; line-height: 30px; padding-top: 15px; margin: 0;}
.pmbox>p i{display:inline-block; width: 22px; height: 20px; background: url(../images/pm-icon.png) center no-repeat; vertical-align: middle; margin-top: -2px; margin-right: 10px;}
.dszxechart{padding: 10px 0 20px;}
.dszxechart p{margin: 0; text-align: right;}
.dszxechart p>span{font-size: 14px; color: #26beff; margin-left: 20px;}
.dszxechart p>span>i{display: inline-block; width: 10px; height: 10px; margin-right: 5px;}
.ys{ background: -webkit-linear-gradient(left, #ffd200 , #ea9809);}
.bs{ background: -webkit-linear-gradient(left, #00e1f9 , rgba(0,225,249.0));}
.dszxechart .echart{width: 297px; height: 353px;}
.gzflqls{width: 220px; height: 150px;}

