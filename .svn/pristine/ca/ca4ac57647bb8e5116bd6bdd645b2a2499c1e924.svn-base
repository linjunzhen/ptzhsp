.eui-table{display: table; width: 100%;}
.eui-table .thead{display: table-header-group;}
.eui-table .tbody{display: table-column-group;}
.eui-table .tfoot{display: table-footer-group;}
.eui-table .tr{display: table-row;}
.eui-table .th{display: table-cell; vertical-align: middle;}
.eui-table .td{display: table-cell; vertical-align: middle;}
.eui-table .min{width: 1%; white-space: nowrap;}
.eui-table.layout-fixed{table-layout: fixed;}
/*动画*/
.eui-wavesM{position: relative; width: 120px; height: 124px; margin: 0 auto; background: url(../images/waves-bg.png) no-repeat center;}
.eui-wavesM:after{content: ""; position: absolute; top: 9px; left: 6px; width: 108px; height: 108px; background: url(../images/nav-qp.gif) no-repeat; background-size: 108px;}
.eui-wavesM .txt{position: absolute; width: 120px; height: 124px; padding-top: 30px; z-index: 10; color: #fff; font-size: 25px; line-height: 30px; text-align: center;}
.eui-wavesM .txt h3{font-size: 68px; line-height: 68px; margin: 0;}
.eui-wavesM .txt h3 span{font-size: 25px; font-weight: 400;}
.eui-wavesM .txt p{margin: 0;}
.eui-wavesM .waves{position: absolute; top: 9px; left: 6px; width: 108px; height: 108px; border-radius: 50%; overflow: hidden;} 
.eui-wavesM .waves .waves-n{position: absolute; left: 0; bottom: 0; width: 200%; height: 100px; background-repeat: no-repeat; background-position: 0 bottom; transform-origin: center bottom; animation: move_wave 10s linear infinite;}
.eui-wavesM .waves .waves-w{position: absolute; left: 0; bottom: 0; width: 200%; height: 100px; background-repeat: no-repeat; background-position: 0 bottom; transform-origin: center bottom; z-index: 1; animation: move_wave 5s linear infinite;}
@keyframes move_wave {
    0% {
        transform: translateX(0) translateZ(0) scaleY(1)
    }
    50% {
        transform: translateX(-25%) translateZ(0) scaleY(0.8)
    }
    100% {
        transform: translateX(-50%) translateZ(0) scaleY(1)
    }
}
