<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>t</short-name>
	<uri>/fda-tag</uri>
	<display-name>"自定义标签"</display-name>
	<tag>
		<name>fda-radiogroup</name>
		<tag-class>net.evecom.core.web.tag.FdaRadioGroup</tag-class>
		<body-content>jsp</body-content>
		<description>自定义radio组</description>
		<attribute>
			<name>width</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>控件宽度</description>
		</attribute>
		<attribute>
			<name>defaultvalue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>默认值</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>回填选中值</description>
		</attribute>
		<attribute>
			<name>typecode</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字典类别编码</description>
		</attribute>
		<attribute>
			<name>fieldname</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字段名称</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>click事件函数名</description>
		</attribute>
	</tag>
	
	<tag>
		<name>fda-eveselect</name>
		<tag-class>net.evecom.core.web.tag.FdaEveSelect</tag-class>
		<body-content>jsp</body-content>
		<description>自定义下拉框</description>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>ID</description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>组件名称</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>回填选中值</description>
		</attribute>
		<attribute>
			<name>clazz</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>组件的样式</description>
		</attribute>
		<attribute>
			<name>onchange</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>onchange事件</description>
		</attribute>
		<attribute>
			<name>datainterface</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据源接口</description>
		</attribute>
		<attribute>
			<name>dataparams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据源接收参数</description>
		</attribute>
		<attribute>
			<name>defaultemptytext</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>缺省提示选择值</description>
		</attribute>
	</tag>
	
	<tag>
		<name>fda-excheckbox</name>
		<tag-class>net.evecom.core.web.tag.FdaExCheckBox</tag-class>
		<body-content>jsp</body-content>
		<description>自定义checkbox组</description>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>ID</description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>组件名称</description>
		</attribute>
		<attribute>
			<name>width</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>组件宽度</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>回填选中值</description>
		</attribute>
		<attribute>
			<name>clazz</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>组件的样式</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>onclick事件</description>
		</attribute>
		<attribute>
			<name>datainterface</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据源接口</description>
		</attribute>
		<attribute>
			<name>queryparamjson</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>动态数据源接口参数JSON</description>
		</attribute>
		<attribute>
			<name>checkvalues</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>静态复选框可选值</description>
		</attribute>
		<attribute>
			<name>checklables</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>静态复选框标签值</description>
		</attribute>
		<attribute>
			<name>datavalidate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>验证表达式</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>disabled</description>
		</attribute>
		<attribute>
			<name>puremode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否纯净模式</description>
		</attribute>
		<attribute>
			<name>isstartp</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>puremode为true并且前后带P</description>
		</attribute>
	</tag>	
	
	<tag>
		<name>fda-exradiogroup</name>
		<tag-class>net.evecom.core.web.tag.FdaExRadioGroup</tag-class>
		<body-content>jsp</body-content>
		<description>自定义radio组</description>
		<attribute>
			<name>width</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>控件宽度</description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>控件命名</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>回填选中值</description>
		</attribute>
		<attribute>
			<name>radiovalues</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>静态单选框可选值</description>
		</attribute>
		<attribute>
			<name>radiolables</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>静态单选框标签值</description>
		</attribute>
		<attribute>
			<name>datainterface</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>动态数据源接口</description>
		</attribute>
		<attribute>
			<name>queryparamjson</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>动态数据源接口参数JSON</description>
		</attribute>
		<attribute>
			<name>allowblank</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否允许为空</description>
		</attribute>
		<attribute>
			<name>puremode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否纯净模式</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>disabled</description>
		</attribute>
		<attribute>
			<name>isstartp</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>puremode为true并且前后带P</description>
		</attribute>
		<attribute>
			<name>selectfirst</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否默认第一个单选框true,false</description>
		</attribute>
	</tag>
	
	<!-- 开始编写自定义下拉框 -->
	<tag>
		<name>fda-exselect</name>
		<tag-class>net.evecom.core.web.tag.FdaExSelect</tag-class>
		<body-content>jsp</body-content>
		<description>自定义下拉框</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>控件命名</description>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>控件ID</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>disabled</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>回填选中值</description>
		</attribute>
		<attribute>
			<name>staticvalues</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>静态可选值</description>
		</attribute>
		<attribute>
			<name>staticlables</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>静态标签值</description>
		</attribute>
		<attribute>
			<name>datainterface</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>动态数据源接口</description>
		</attribute>
		<attribute>
			<name>queryparamjson</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>动态数据源接口参数JSON</description>
		</attribute>
		<attribute>
			<name>defaultemptytext</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>缺省选择提示,例如(请选择性别)</description>
		</attribute>
		<attribute>
			<name>allowblank</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否允许为空</description>
		</attribute>
		<attribute>
			<name>clazz</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>样式</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>style样式</description>
		</attribute>
	</tag>
	<!-- 结束编写自定义下拉框 -->
	
	
</taglib>