.eui-content{width: 224px;height: 100%;overflow: hidden;}
.jquery-accordion-menu,.jquery-accordion-menu * {
	font-family: "Microsoft YaHei";
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	outline: 0
}

.jquery-accordion-menu {
	min-width: 224px;
	float: left;
	height:100%;
	position: relative;
}
.jquery-accordion-menu ul {
	margin: 0;
	padding: 0;
	list-style: none
}
.jquery-accordion-menu ul li {
	width: 100%;
	display: block;
	float: left;
	position: relative;
}
.jquery-accordion-menu ul li a {
	width: 100%;
	padding: 10px 22px;
	float: left;
	text-decoration: none;
	color: ##014781;
	font-size: 14px;
	background: #fdfeff;
	white-space: nowrap;
	position: relative;
	overflow: hidden;
	/* border-bottom:1px solid #cbdcf5; */
	-o-transition: color .2s linear,background .2s linear;
	-moz-transition: color .2s linear,background .2s linear;
	-webkit-transition: color .2s linear,background .2s linear;
	transition: color .2s linear,background .2s linear;
	box-sizing: border-box;
}
.jquery-accordion-menu ul li a:hover{
	color:#fff;
	background: #3e8bff;
}
.jquery-accordion-menu ul .eui-active1 a{color: #014781;/* background: #3e8bff; */}

/* .jquery-accordion-menu>ul>li.active>a,.jquery-accordion-menu>ul>li:hover>a {
	color: #427091;
	background: #ebeff8
} */

.jquery-accordion-menu>ul>li .jquery-accordion-submenu li.eui-active>a{
	 background: url(../images/sub-up1-on.png) no-repeat 22px #3e8bff !important;
	 color:#fff !important;
}
.jquery-accordion-menu ul ul.jquery-accordion-submenu>li>ul.jquery-accordion-submenu>li.eui-active>a {
	background:#237ae4;
}
/* .jquery-accordion-menu>ul>li>a {
	border-bottom: solid 1px #2f313d
} */

.jquery-accordion-menu ul li a i {
	width: 22px;
	/**float: left;**/
	line-height: 18px;
	font-size: 16px;
	text-align: left
}

.jquery-accordion-menu .submenu-indicator {
	top:14px;
	float: right;
	right: 22px;
	position: absolute;
	line-height: 19px;
	font-size: 20px;
	display:inline-block;
	width: 12px;
	height: 12px;
	background: url(../images/sub-up.png) no-repeat;
	/* -o-transition: transform .3s linear;
	-moz-transition: transform .3s linear;
	-webkit-transition: transform .3s linear;
	-ms-transition: transform .3s linear */
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu .submenu-indicator {
	line-height: 16px;
	top:10px;
	background:url(../images/menu.png) no-repeat !important;
}

.jquery-accordion-menu .submenu-indicator-minus>.submenu-indicator {
	/* -ms-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg) */
	background: url(../images/sub-down.png) no-repeat;
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu,.jquery-accordion-menu ul ul.jquery-accordion-submenu li ul.jquery-accordion-submenu {
	width: 100%;
	display: none;
	position: relative;
	/* overflow: auto; */
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu li {
	clear: both;
	width: 100%
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu li a {
	width: 100%;
	float: left;
	font-size: 14px;
	padding:6px 22px;
	background: url(../images/sub-up1.png) no-repeat 22px #f2f6ff;
	color: #014781;
	border-top: none;
	border-bottom: none;
	position: relative;
	-o-transition: border .2s linear;
	-moz-transition: border .2s linear;
	-webkit-transition: border .2s linear;
	transition: border .2s linear;
	box-sizing: border-box;
	line-height: 24px;
}
.jquery-accordion-menu ul ul.jquery-accordion-submenu li a:hover{
	color:#fff;
	background: url(../images/sub-up1-on.png) no-repeat 22px #3e8bff !important;
}
.jquery-accordion-menu ul ul.jquery-accordion-submenu>li>a {
	padding-left: 40px;
	
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu>li>ul.jquery-accordion-submenu>li>a {
	padding-left: 40px;
	border-bottom:1px solid #c5d2e2;
	background:#d6deeb;
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu>li>ul.jquery-accordion-submenu>li>ul.jquery-accordion-submenu>li>a {
	padding-left: 60px;
}

.jquery-accordion-menu ul li .jquery-accordion-menu-label,.jquery-accordion-menu ul ul.jquery-accordion-submenu li .jquery-accordion-menu-label {
	min-width: 20px;
	padding: 1px 2px 1px 1px;
	position: absolute;
	right: 18px;
	top: 10px;
	font-size: 11px;
	font-weight: 800;
	color: #fff;
	text-align: center;
	line-height: 18px;
	background: #f05050;
	border-radius: 100%
}

.jquery-accordion-menu ul ul.jquery-accordion-submenu li .jquery-accordion-menu-label {
	top: 6px
}

.ink {
	display: block;
	position: absolute;
	background: rgba(255,255,255,.3);
	border-radius: 100%;
	-webkit-transform: scale(0);
	-moz-transform: scale(0);
	-ms-transform: scale(0);
	-o-transform: scale(0);
	transform: scale(0)
}

.animate-ink {
	-webkit-animation: ripple .5s linear;
	-moz-animation: ripple .5s linear;
	-ms-animation: ripple .5s linear;
	-o-animation: ripple .5s linear;
	animation: ripple .5s linear
}

@-webkit-keyframes ripple {
	100% {
		opacity: 0;
		-webkit-transform: scale(2.5)
	}
}

@-moz-keyframes ripple {
	100% {
		opacity: 0;
		-moz-transform: scale(2.5)
	}
}

@-o-keyframes ripple {
	100% {
		opacity: 0;
		-o-transform: scale(2.5)
	}
}

@keyframes ripple {
	100% {
		opacity: 0;
		transform: scale(2.5)
	}
}
