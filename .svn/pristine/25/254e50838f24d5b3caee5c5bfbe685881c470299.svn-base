/*------------------------- 焦点图 ------------------------*/
.fl{width: 630px; height: 400px; background-color: #fff; float: left; position: relative; overflow: hidden;}
.fl .pic img{width: 630px; height: 400px; display: block; }
.fl .txt-bg{position: absolute; bottom: 0; z-index: 1; height: 36px; width: 100%; background: #000; filter: alpha(opacity=50); opacity: 0.5; overflow: hidden; }
.fl .txt{position: absolute; bottom: 0; z-index: 2; height: 36px; width: 100%; overflow: hidden; }
.fl .txt li{height: 36px; line-height: 36px; position: absolute; bottom: -36px; }
.fl .txt li a{    display: block;     color: white;     padding: 0 0 0 14px;     font-size: 14px;     text-decoration: none; }
.fl .num{position: absolute; z-index: 3; bottom: 15px; right: 8px; }
.fl .num li{float: left; position: relative; width: 28px; height: 5px; overflow: hidden; margin-right: 7px; cursor: pointer; }
.fl .num li a, .fl .num li span{    position: absolute;     z-index: 2;     display: block;     width: 100%;     height: 100%;     top: 0;     left: 0;     text-decoration: none; }
.fl .num li span{    z-index: 1;     background: #a5a5a5; }
.fl .num li.on a, .fl .num a:hover{    background: #1a88d4; }
