@charset "utf-8";
/* CSS Document */
html,body,div,form,img,ul,ol,li,dl,dt,dd{margin:0px; padding:0px;}
body{margin:0 auto;padding:0;border:0;}
select,input,textarea{vertical-align:middle; padding:0; margin:0;}
textarea{resize:none;}
img{border:0;}
ol,ul,li{list-style:none;}
a{color: #475056; text-decoration:none; cursor: pointer;}
a,img,input,textarea,select,button,area{outline:none;}

.lfloat{float:left;}
.rfloat{float:right;}
/*清除浮动*/
.clearfix:after{content:'\0020';display:block;height:0;clear:both;visibility:hidden;}
.clearfix{*zoom:1; _zoom:1;}

/* common */
a {color: #444444;}
body{font-size: 12px; font-family: "微软雅黑", Microsoft YaHei; color: #333; margin: 0; background-color: #f4fcff;}

.center{margin: 0 auto;}
.clear{clear: both; height: 1px;}
/* head */
.head{width: 100%; height: 221px; background: url(../images/head_bg.jpg) left top repeat-x;}
.top_nav{width: 1000px; height: 30px; background-color: #f9f9f9; color: #333;}
.wel_icon{width: 20px; height: 18px; float: left; margin-top: 6px;}
.wel{height: 30px; float: left; line-height: 30px;}
.top_nav_right{width: 38%; height: 30px; float: right;}
.top_nav_line{color: #ccc;}
.top_nav_right ul{margin: 0; padding: 0;}
.top_nav_right li{margin-left: 10px; line-height: 30px; float: right;}
.top_nav_right li a{color: #333; }
.lgsh{width: 1000px; height: 151px; background: url(../images/logo.jpg) left top no-repeat;}
.logo{width: 520px; height: 64px; padding-top: 43px; float: left;}

.search{width: 310px; height: 40px; float: right; padding-top: 56px;}
.search input{width: 210px; height: 14px; padding-top: 9px; padding-bottom: 9px; padding-left: 10px; padding-right: 10px; border: none; outline: none; float: right; color: #999; }
.search a{float: right; width: 60px; height: 32px; font-family: "微软雅黑", Microsoft YaHei; font-size: 14px; color: #fff; background-color: #1a88d4; text-align: center; line-height: 32px; }

.navigater{width: 1000px; height: 40px;}
.navigater ul{margin: 0; padding: 0; }
.navigater li{width: 111px; float: left; line-height: 40px; text-align: center; background: url(../images/nav_line.jpg) right no-repeat; }
.navigater li a{font-size: 16px; color: #fff; display: block; width: 111px; }
.navigater li a:hover{background-color: #2395e4; }

.main{margin-top: 20px; width: 1000px; overflow: hidden; /* IE8 */}

/*  可贷楼盘 */
.list .kdlp{width: 700px; height: 100%; float: left; margin-left: 10px; margin-top: 10px;}
.list .kdlp li{width: 200px; height: 245px; float: left; position: relative; margin-left: 30px; margin-top: 15px; }
.list .kdlp li .cover{display: block; width: 200px; height: 32px; background-color: #000; position: absolute; top: 108px; opacity: 0.5; filter: alpha(opacity=50); /* 针对 IE8 以及更早的版本 */     }
.list .kdlp li .mz{position: absolute; top: 108px; color: #fff; font-size: 14px; margin-left: 10px; line-height: 32px; }
.list .kdlp li a{display: block; width: 200px; height: 140px; }
.list .kdlp li div{width: 200px; height: 104px; line-height: 24px; margin-top: 10px; }
/*-------------------------列表----------------------------*/
.list{margin-top: 20px; width: 1000px; height: 880px; background-color: #FFF;}
.list .lf{width: 240px; float: left; padding-left: 20px; padding-top: 20px; }
.list .lf img{margin-left: 18px; margin-top: 17px; margin-right: 11px; }
.list .lf .tl{width: 240px; height: 46px; background-color: #1a88d4; font-size: 16px; color: #fff; }
.list .lf ul{width: 240px; }
.list .lf ul li{border-bottom: 2px solid #fff; line-height: 40px; font-size: 14px; width: 240px; height: 40px; color: #666; font-weight: normal; background-color: #eeeeee; }
.list .lf ul li a{display: block; width: 240px; height: 40px;}
.list .lf ul li a div{width: 6px; height: 6px; background: url(../images/1.jpg); float: left;             margin-top: 15px; margin-left: 18px; margin-right: 12px; }
.list .lf ul li a:hover{background-color: #ff8400; color: #fff;}
.list .lf ul li a:hover div{background: url(../images/1.jpg);}
.list .rf{width: 740px; float: left; }
.list .rf .lst_tl{float: left; font-size: 16px; font-weight: bold; border-bottom: 2px solid #1a88d4; width: 180px; text-align: center; height: 46px; line-height: 55px; color: #1a88d4; }
.list .rf .position{margin-top: 20px; float: right; }
.list .rf .lst_item{width: 700px; height: 695px; float: left; margin-left: 20px; margin-top: 10px; }
.list .rf .lst_item li{line-height: 30px;     font-size: 14px;     background-image: url(../images/1.jpg);     background-size:6px;     background-repeat: no-repeat;     background-position: left center;     text-indent: 20px;     color: #666; }
.list .rf .lst_item li a:hover{    color: #1a88d4;     }
.list .rf .lst_item li span{    float: right;         font-size: 14px;         color: #999;     }
.list .rf .pager{width: 564px; height: 45px; /*background-image:url(../images/pager.jpg); float:left;	margin-left:343px; border-top:1px solid #e8e8e8;*/margin-top: 10px; padding-top: 15px; }
