.title{width: 100px; height: 33px; line-height: 32px; background-color: #1a88d4; color: #FFF; font-size: 15px; font-weight: bold; text-align: center; margin-top: 10px; margin-left: 10px; float: left;}

.fl_new{width: 350px; height: 400px; background-color: #fff; float: left; margin-left: 20px; font-size: 14px; position: relative;}
.fl_new .inHd{height: 42px; line-height: 42px; border-bottom: 2px solid #1a88d4; margin-left: 10px; margin-right: 10px; }
.fl_new .more{display: block; font-size: 12px; position: absolute; left: 305px; top: -0px; color: #999; }
.fl_new .inHd ul{overflow: hidden; zoom: 1; }
.fl_new .inHd ul li{height: 42px; width: 74px; text-align: center; float: left; cursor: pointer; margin-left:10px; background: #eeeeee;     color: #666666; }
.fl_new .inHd ul li.on{    background: #ccc;     color: #fff; }
.fl_new .inBd ul{padding: 15px; zoom: 1; overflow: hidden; }
.fl_new .inBd li{overflow: hidden; }
.inBd li a{display: block; padding: 0 4px; height: 28px; line-height: 28px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.inBd li a:before{content: ''; margin: -2px 8px 0 0; display: inline-block; vertical-align: middle; width: 6px; height: 6px; background: #1a88d5;}
.fl_new .inBd li a:hover{color: #1a88d4; }
.fl_new .inBd li .date{float: right; color: #999; }


