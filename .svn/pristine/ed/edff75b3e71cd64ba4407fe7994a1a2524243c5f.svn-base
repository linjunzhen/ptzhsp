.eui-9-progress{
	
}
.eui-9-progress-head{
	margin: 20px  30px 0 30px;
    width: 300px;
    height: 36px;
    line-height: 36px;
    color: #FFFFFF;
    font-size: 18px;
    background: url(../images/index3.png) no-repeat;
    background-size: 100%;
    padding-left: 20px;
}
.eui-9-progress-input{
	margin-top: 18px;
	position: relative;
}
.eui-9-progress-input table{
	padding: 0 60px;
	display: inline-block;
}
.eui-9-progress-input tr{
	display: inline-block;
	margin-top: 6px;
}
.eui-9-progress-input tr td{
	height: 37px;
	line-height: 37px;
}
.eui-9-progress-input table tr td:nth-child(odd) {
    width: 140px;
    text-align: right;
    padding-right: 10px;
    vertical-align: inherit;
}
.eui-9-progress-input tr td input{
	width: 350px;
    height: 35px;
    line-height: 35px;
    border: solid 1px #e1e6eb;
}
.eui-9-progress-input table tr td select{
	width: 352px;
	height: 37px;
	line-height: 37px;
	border: solid 1px #e1e6eb;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	background: url(../images/down.png) no-repeat right,#FFFFFF;
	background-position-x: 320px;
	padding-left: 10px;
}
.eui-9-progress-input table tr .eui-9-progress-data select{
	 width: 160px;
}
.eui-9-progress-input table tr .eui-9-progress-data span{
	display: inline-block;
	width: 23px;
	text-align: center;
}
.eui-9-progress-btn{
	position: absolute;
	bottom: 0;
	left: 50%;
	transform: translate(-50%);
	display: inline-block;
	width: 90px;
	height: 36px;
	text-align: center;
	line-height: 36px;
	font-size: 16px;
	color: #FFFFFF;
	background: #0f88ec;
	border-radius: 5px;
	margin-top: 16px;
}
.eui-query-btn{
	bottom: 0;
	display: inline-block;
	width: 90px;
	height: 36px;
	text-align: center;
	line-height: 36px;
	font-size: 16px;
	color: #FFFFFF;
	background: #0f88ec;
	border-radius: 5px;
/* 	margin-top: 16px; */
}
.eui-9-progress-table-1{
	margin:60px 30px 30px 30px;
}
.eui-9-progress-table-1 table{
	width: 100%;
}
.eui-9-progress-table-1 table tr:first-child{
	background: -webkit-linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* 标准的语法 */
}
.eui-9-progress-table-1 table tr{
	text-align: center;
}
.eui-9-progress-table-1 table tr th{
	height: 41px;
	line-height: 41px;
	color: #FFFFFF;
	font-size: 16px;
	border: solid 1px #e1e1e1;
}
.eui-9-progress-table-1 table tr td{
	height: 60px;
	line-height: 30px;
	color: #4c4c4c;
	font-size: 16px;
	border: solid 1px #e1e1e1;
}
.eui-9-progress-table-1 table tr td .span-1{
	color:#01b549 ;
}
.eui-9-progress-table-1 table tr td .span-2{
	color:#f40000 ;
}
.eui-9-progress-table-3{
	margin: 30px;
}
.eui-9-progress-table-3 table{
	width: 100%;
}
.eui-9-progress-table-3 table tr{
	height: 60px;
	line-height: 60px;
}
.eui-9-progress-table-3 table tr td{
	font-size: 14px;
	border:solid 1px #e1e1e1;
}
.eui-9-progress-table-3 table tr td:nth-child(odd){
	width: 184px;
	text-align: right;
	padding-right: 16px;
	color: #FFFFFF;
	background: -webkit-linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right,rgba(15,136,236,0.8), rgba(62,158,238,0.8)); /* 标准的语法 */
}
.eui-9-progress-table-3 table tr td:nth-child(even){
	padding-left: 20px;
}
.czxq{
	color: #0078ff;
	text-decoration: underline;
}























































































































